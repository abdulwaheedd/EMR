@{
    ViewData["Title"] = "All Dashboards";
    ViewData["PageId"] = "All Dashboards";
}
<!DOCTYPE html>
<html>
<head>
    <title>Annual trend dashboard</title>
    <link href="@Url.Content("~/themes/default-theme/ej.dashboardViewer.all.min.css")" rel="stylesheet">
    @*<script src="@Url.Content("~/scripts/jquery-1.10.2.min.js")"></script>*@
    <script src="@Url.Content("~/scripts/jquery.easing.1.3.min.js")"></script>
    <script src="@Url.Content("~/scripts/ej.dashboardViewer.all.min.js")"></script>
    @*<script src="@Url.Content("~/scripts/ej.unobtrusive.min.js")"></script>*@


</head>
<body>
    <div style="height:100%;width:100%;">
        <div style="height:100%;float:left;width:100%;border: 1px solid #D9DEDD;background-color: white;overflow:hidden !important;">
            <div style="height:700px;width:100%;overflow: hidden !important;" id="dashboard"> </div>
        </div>
    </div>
</body>
</html>
<script type="text/javascript">

    $("#dashboard").ejDashboardViewer(   // Add the ejDashboardViewer reference to render the dashboard
         {
             serviceUrl: '@ViewBag.ServiceUrl.ToString()',
             dashboardPath: '@ViewBag.DashboardPath',
             filterParameters: location.search.substring(1)
         });
</script> 